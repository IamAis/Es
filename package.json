  "build": {
    "appId": "com.app-fatture.desktop",
    "productName": "App Fatture",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
